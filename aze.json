{
    "name": "ppx-figma",
    "version": "0.1.5",
    "description": "Ppx-figma import styles from Figma and generate css based on Bs-Css",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/JulesGuesnon/Ppx-Figma"
    },
    "author": "Jules Guesnon <hello@julesguesnon.com>",
    "keywords": ["ReasonML", "Reason", "Figma", "Ppx", "Bs-Css"],
    "esy": {
        "build": "dune build -p #{self.name}",
        "buildsInSource": "_build",
        "release": {
            "releasedBinaries": ["ppx-figma"]
        }
    },
    "scripts": {
        "run": "esy x ppx-figma"
    },
    "dependencies": {
        "ocaml": "4.10.0",
        "@esy-ocaml/reason": "^3.5.2",
        "@opam/ocaml-migrate-parsetree": "^1.7.3",
        "@opam/ppxlib": "0.14.0",
        "@opam/tls": "*",
        "@opam/dune": "*",
        "@opam/reason": "*",
        "@opam/cohttp": "*",
        "@opam/cohttp-lwt-unix": "*",
        "@opam/lwt": "*",
        "@opam/yojson": "*"
    },
    "devDependencies": {
        "@opam/merlin": "*",
        "@opam/jbuilder": "*"
    },
    "resolutions": {
        "@esy-ocaml/reason": "facebook/reason:reason.json#773dbcd"
    }
}
