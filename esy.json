{
    "name": "ppx-figma",
    "version": "0.2.1",
    "description": "Ppx-figma import styles from Figma and generate css based on Bs-Css",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/JulesGuesnon/Ppx-Figma"
    },
    "author": "Jules Guesnon <hello@julesguesnon.com>",
    "keywords": ["ReasonML", "Reason", "Figma", "Ppx", "Bs-Css"],
    "esy": {
        "build": "dune build -p #{self.name}",
        "buildsInSource": "_build",
        "release": {
            "releasedBinaries": ["ppx-figma"]
        }
    },
    "scripts": {
        "run": "esy x ppx-figma"
    },
    "dependencies": {
        "ocaml": "4.6.1003",
        "@opam/ppxlib": "*",
        "@opam/tls": "*",
        "@opam/dune": "*",
        "@opam/reason": "*",
        "@opam/cohttp": "*",
        "@opam/cohttp-lwt-unix": "2.5.4",
        "@opam/lwt": "*",
        "@opam/yojson": "*",
        "@opam/num": "*",
        "@opam/cstruct-sexp": "5.2.0",
        "@opam/cstruct-lwt": "5.2.0"
    },
    "devDependencies": {
        "@opam/merlin": "*",
        "@opam/jbuilder": "*"
    }
}
